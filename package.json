{
  "name": "hahaki",
  "description": "Download to Image of opened image tab",
  "scripts": {
    "zip": "zip hahaki.zip -r app",
    "css": "stylus -w -o app/style/ app/style/*.styl",
    "js": "coffee -bw -o app/script app/script/*.coffee",
    "dev": "concurrently --kill-others 'yarn js' 'yarn css'"
  },
  "repository": "nju33/chrome-hahaki",
  "author": "nju33 <nju33.ki@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/totora0155/hahaki#readme",
  "devDependencies": {
    "coffee-script": "1.10.0",
    "stylus": "0.52.4"
  }
}
